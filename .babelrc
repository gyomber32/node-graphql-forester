{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": false
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        [
            "transform-inline-environment-variables",
            {
                "include": [
                    "NODE_ENV"
                ]
            }
        ]
    ]
}